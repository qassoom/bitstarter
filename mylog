[31m-  // end[m
[31m-  if (!part) {[m
[31m-    if (Array.isArray(parent[key])) {[m
[31m-      parent[key].push(val);[m
[31m-    } else if ('object' == typeof parent[key]) {[m
[31m-      parent[key] = val;[m
[31m-    } else if ('undefined' == typeof parent[key]) {[m
