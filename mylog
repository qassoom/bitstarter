[32m+[m[32mfor (var i = 0; i < 25; i++) {[m
[32m+[m[32m    var dir = Math.floor(Math.random() * Math.pow(16,4)).toString(16);[m
[32m+[m[32m    ps.push(dir);[m
[32m+[m[32m}[m
